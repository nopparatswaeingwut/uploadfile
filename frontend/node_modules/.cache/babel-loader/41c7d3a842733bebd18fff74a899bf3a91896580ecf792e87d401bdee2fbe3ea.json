{"ast":null,"code":"export default {\n  name: 'UploadPage',\n  data() {\n    return {\n      selectedFiles: [],\n      uploadedFiles: []\n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      const files = Array.from(event.target.files);\n      if (this.selectedFiles.length + files.length > 10) {\n        alert('คุณสามารถอัพโหลดไฟล์ได้สูงสุด 10 ไฟล์เท่านั้น');\n        return;\n      }\n      this.selectedFiles = [...this.selectedFiles, ...files];\n    },\n    handleDrop(event) {\n      const files = Array.from(event.dataTransfer.files);\n      if (this.selectedFiles.length + files.length > 10) {\n        alert('คุณสามารถอัพโหลดไฟล์ได้สูงสุด 10 ไฟล์เท่านั้น');\n        return;\n      }\n      this.selectedFiles = [...this.selectedFiles, ...files];\n    },\n    isImage(file) {\n      return file && file['type'].split('/')[0] === 'image';\n    },\n    isVideo(file) {\n      return file && file['type'].split('/')[0] === 'video';\n    },\n    truncateFileName(fileName) {\n      return fileName.length > 10 ? fileName.slice(0, 10) + '...' : fileName;\n    },\n    async submitFiles() {\n      if (!this.selectedFiles.length) {\n        alert('Please select files to upload.');\n        return;\n      }\n      const formData = new FormData();\n      this.selectedFiles.forEach(file => {\n        formData.append('files[]', file);\n      });\n      try {\n        const response = await fetch('http://localhost:3000/upload', {\n          method: 'POST',\n          body: formData\n        });\n        if (response.ok) {\n          const result = await response.json();\n          alert('Files uploaded successfully: ' + result.files.join(', '));\n          this.selectedFiles = [];\n          this.fetchUploadedFiles(); // เรียกฟังก์ชันเพื่อดึงข้อมูลไฟล์ใหม่\n        } else {\n          const errorResponse = await response.json();\n          alert('File upload failed: ' + errorResponse.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('File upload failed due to a network or server error.');\n      }\n    },\n    async fetchUploadedFiles() {\n      try {\n        const response = await fetch('http://localhost:3000/files');\n        if (response.ok) {\n          this.uploadedFiles = await response.json(); // เก็บข้อมูลไฟล์ในตัวแปร uploadedFiles\n        } else {\n          console.error('Failed to fetch uploaded files');\n        }\n      } catch (error) {\n        console.error('Error fetching uploaded files:', error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchUploadedFiles(); // เรียกฟังก์ชันเมื่อคอมโพเนนต์ถูกสร้าง\n  }\n};","map":{"version":3,"names":["name","data","selectedFiles","uploadedFiles","methods","handleFileUpload","event","files","Array","from","target","length","alert","handleDrop","dataTransfer","isImage","file","split","isVideo","truncateFileName","fileName","slice","submitFiles","formData","FormData","forEach","append","response","fetch","method","body","ok","result","json","join","fetchUploadedFiles","errorResponse","message","error","console","mounted"],"sources":["C:\\Users\\sodat\\Desktop\\upload\\frontend\\src\\components\\uploadPage.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <h2>Upload Files</h2>\n    <div \n      class=\"border border-dashed p-5 text-center mb-3\" \n      @dragover.prevent \n      @drop.prevent=\"handleDrop\"\n      @click=\"$refs.fileInput.click()\"\n    >\n      <p>ลากไฟล์ไปที่นี่ หรือ <strong>คลิกเพื่อเลือกไฟล์</strong></p>\n      <input \n        type=\"file\" \n        class=\"d-none\" \n        ref=\"fileInput\" \n        @change=\"handleFileUpload\" \n        multiple\n      >\n      \n      <div class=\"preview-area mt-3\">\n        <div class=\"file-preview-container\">\n          <div v-for=\"(file, index) in selectedFiles\" :key=\"index\" class=\"file-preview text-center\">\n            <div v-if=\"isImage(file)\">\n              <i class=\"bi bi-file-image\" style=\"font-size: 50px;\"></i>\n            </div>\n            <div v-else-if=\"isVideo(file)\">\n              <i class=\"bi bi-file-play\" style=\"font-size: 50px;\"></i>\n            </div>\n            <div v-else>\n              <i class=\"bi bi-file-earmark\" style=\"font-size: 50px;\"></i>\n            </div>\n            <p>{{ truncateFileName(file.name) }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <button class=\"btn btn-primary\" @click=\"submitFiles\" :disabled=\"!selectedFiles.length\">อัพโหลดไฟล์</button>\n\n    <h2 class=\"mt-5\">Files</h2>\n    <table class=\"table table-bordered mt-3\">\n      <thead>\n        <tr>\n          <th>#</th>\n          <th>File Name</th>\n          <th>Upload Date</th>\n          <th>URL</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(file, index) in uploadedFiles\" :key=\"index\">\n          <td>{{ index + 1 }}</td>\n          <td>{{ file.filename }}</td>\n          <td>{{ new Date(file.uploadDate).toLocaleString() }}</td>\n          <td><a :href=\"file.url\" target=\"_blank\">View</a></td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'UploadPage',\n  data() {\n    return {\n      selectedFiles: [],\n      uploadedFiles: [] \n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      const files = Array.from(event.target.files);\n      if (this.selectedFiles.length + files.length > 10) {\n        alert('คุณสามารถอัพโหลดไฟล์ได้สูงสุด 10 ไฟล์เท่านั้น');\n        return;\n      }\n      this.selectedFiles = [...this.selectedFiles, ...files];\n    },\n    handleDrop(event) {\n      const files = Array.from(event.dataTransfer.files);\n      if (this.selectedFiles.length + files.length > 10) {\n        alert('คุณสามารถอัพโหลดไฟล์ได้สูงสุด 10 ไฟล์เท่านั้น');\n        return;\n      }\n      this.selectedFiles = [...this.selectedFiles, ...files];\n    },\n    isImage(file) {\n      return file && file['type'].split('/')[0] === 'image';\n    },\n    isVideo(file) {\n      return file && file['type'].split('/')[0] === 'video';\n    },\n    truncateFileName(fileName) {\n      return fileName.length > 10 ? fileName.slice(0, 10) + '...' : fileName;\n    },\n    async submitFiles() {\n      if (!this.selectedFiles.length) {\n        alert('Please select files to upload.');\n        return;\n      }\n\n      const formData = new FormData();\n      this.selectedFiles.forEach(file => {\n        formData.append('files[]', file);\n      });\n\n      try {\n        const response = await fetch('http://localhost:3000/upload', {\n          method: 'POST',\n          body: formData\n        });\n\n        if (response.ok) {\n          const result = await response.json();\n          alert('Files uploaded successfully: ' + result.files.join(', '));\n          this.selectedFiles = [];\n          this.fetchUploadedFiles(); // เรียกฟังก์ชันเพื่อดึงข้อมูลไฟล์ใหม่\n        } else {\n          const errorResponse = await response.json();\n          alert('File upload failed: ' + errorResponse.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('File upload failed due to a network or server error.');\n      }\n    },\n    async fetchUploadedFiles() {\n      try {\n        const response = await fetch('http://localhost:3000/files');\n        if (response.ok) {\n          this.uploadedFiles = await response.json(); // เก็บข้อมูลไฟล์ในตัวแปร uploadedFiles\n        } else {\n          console.error('Failed to fetch uploaded files');\n        }\n      } catch (error) {\n        console.error('Error fetching uploaded files:', error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchUploadedFiles(); // เรียกฟังก์ชันเมื่อคอมโพเนนต์ถูกสร้าง\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 600px;\n}\n.file-preview-container {\n  display: flex;\n  overflow-x: auto; \n  scrollbar-width: thin;\n}\n.file-preview {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-right: 30px; \n  margin-top: 0;\n}\n.preview-area {\n  max-height: 130px; \n}\n</style>\n"],"mappings":"AA6DA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,MAAMC,KAAI,GAAIC,KAAK,CAACC,IAAI,CAACH,KAAK,CAACI,MAAM,CAACH,KAAK,CAAC;MAC5C,IAAI,IAAI,CAACL,aAAa,CAACS,MAAK,GAAIJ,KAAK,CAACI,MAAK,GAAI,EAAE,EAAE;QACjDC,KAAK,CAAC,+CAA+C,CAAC;QACtD;MACF;MACA,IAAI,CAACV,aAAY,GAAI,CAAC,GAAG,IAAI,CAACA,aAAa,EAAE,GAAGK,KAAK,CAAC;IACxD,CAAC;IACDM,UAAUA,CAACP,KAAK,EAAE;MAChB,MAAMC,KAAI,GAAIC,KAAK,CAACC,IAAI,CAACH,KAAK,CAACQ,YAAY,CAACP,KAAK,CAAC;MAClD,IAAI,IAAI,CAACL,aAAa,CAACS,MAAK,GAAIJ,KAAK,CAACI,MAAK,GAAI,EAAE,EAAE;QACjDC,KAAK,CAAC,+CAA+C,CAAC;QACtD;MACF;MACA,IAAI,CAACV,aAAY,GAAI,CAAC,GAAG,IAAI,CAACA,aAAa,EAAE,GAAGK,KAAK,CAAC;IACxD,CAAC;IACDQ,OAAOA,CAACC,IAAI,EAAE;MACZ,OAAOA,IAAG,IAAKA,IAAI,CAAC,MAAM,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,MAAM,OAAO;IACvD,CAAC;IACDC,OAAOA,CAACF,IAAI,EAAE;MACZ,OAAOA,IAAG,IAAKA,IAAI,CAAC,MAAM,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,MAAM,OAAO;IACvD,CAAC;IACDE,gBAAgBA,CAACC,QAAQ,EAAE;MACzB,OAAOA,QAAQ,CAACT,MAAK,GAAI,EAAC,GAAIS,QAAQ,CAACC,KAAK,CAAC,CAAC,EAAE,EAAE,IAAI,KAAI,GAAID,QAAQ;IACxE,CAAC;IACD,MAAME,WAAWA,CAAA,EAAG;MAClB,IAAI,CAAC,IAAI,CAACpB,aAAa,CAACS,MAAM,EAAE;QAC9BC,KAAK,CAAC,gCAAgC,CAAC;QACvC;MACF;MAEA,MAAMW,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/B,IAAI,CAACtB,aAAa,CAACuB,OAAO,CAACT,IAAG,IAAK;QACjCO,QAAQ,CAACG,MAAM,CAAC,SAAS,EAAEV,IAAI,CAAC;MAClC,CAAC,CAAC;MAEF,IAAI;QACF,MAAMW,QAAO,GAAI,MAAMC,KAAK,CAAC,8BAA8B,EAAE;UAC3DC,MAAM,EAAE,MAAM;UACdC,IAAI,EAAEP;QACR,CAAC,CAAC;QAEF,IAAII,QAAQ,CAACI,EAAE,EAAE;UACf,MAAMC,MAAK,GAAI,MAAML,QAAQ,CAACM,IAAI,CAAC,CAAC;UACpCrB,KAAK,CAAC,+BAA8B,GAAIoB,MAAM,CAACzB,KAAK,CAAC2B,IAAI,CAAC,IAAI,CAAC,CAAC;UAChE,IAAI,CAAChC,aAAY,GAAI,EAAE;UACvB,IAAI,CAACiC,kBAAkB,CAAC,CAAC,EAAE;QAC7B,OAAO;UACL,MAAMC,aAAY,GAAI,MAAMT,QAAQ,CAACM,IAAI,CAAC,CAAC;UAC3CrB,KAAK,CAAC,sBAAqB,GAAIwB,aAAa